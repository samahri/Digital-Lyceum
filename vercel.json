{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "functions": {
    "api/server.js": {
      "runtime": "@vercel/node",
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/server" }
  ]
}
